<body>

  <div class="container">

    <form class="form-horizontal">
      <fieldset>


        <!-- change col-sm-N to reflect how you would like your column spacing (http://getbootstrap.com/css/#forms-control-sizes) -->


        <!-- Form Name -->
        <legend>Gather Information</legend>

        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="website" class="control-label col-sm-2">Website</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="website" placeholder="Website URL">

          </div>
        </div>
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="company" class="control-label col-sm-2">Company</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="company" placeholder="Company Name">

          </div>
        </div>
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="address" class="control-label col-sm-2">Address</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="address" placeholder="Address">

          </div>
        </div>
        
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="address" class="control-label col-sm-2">State / Region</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="state" placeholder="State">

          </div>
        </div>
        
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="address" class="control-label col-sm-2">Zip Code</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="zip" placeholder="Zip Code">

          </div>
        </div>
        
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="address" class="control-label col-sm-2">Country</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="country" placeholder="Country">

          </div>
        </div>
        
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="phone" class="control-label col-sm-2">Phone</label>
          <div class="col-sm-10">
            <input type="tel" class="form-control" id="phone" placeholder="Company Phone">

          </div>
        </div>
        <!-- Text input http://getbootstrap.com/css/#forms -->
        <div class="form-group">
          <label for="email" class="control-label col-sm-2">Email</label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="email" placeholder="Company Email">

          </div>
        </div>
        <!-- Textarea http://getbootstrap.com/css/#textarea -->
        <div class="form-group">
          <label class="control-label col-sm-2" for="notes">Notes</label>
          <div class="col-sm-10">
            <textarea class="form-control" id="notes" name="notes" rows="3"></textarea>

          </div>
        </div>

        <!-- Button Group http://getbootstrap.com/components/#btn-groups -->
        <div class="form-group">
          <label class="control-label col-sm-2"></label>
          <div class="text-right col-sm-10">
            <div id="button1idGroup" class="btn-group" role="group" aria-label="">
              <!-- <button type="button" id="button1id" name="button1id" class="btn btn-success" aria-label="Save">Save</button> -->
              <button type="reset" id="button2id" name="button2id" class="btn btn-danger" aria-label="Save">Clear</button>
            </div>

          </div>
        </div>


      </fieldset>
    </form>


  </div>

</body>

</html>